{
  "name": "rd-frontend-vue3-web",
  "private": true,
  "version": "1.0.0",
  "templateVersion": "1.0.8",
  "type": "module",
  "scripts": {
    "dev": "vite --mode development",
    "dev:staging": "vite --mode staging",
    "dev:prod": "vite --mode production",
    "build:dev": "vue-tsc -b && vite build --mode development",
    "build:staging": "vue-tsc -b && vite build --mode staging",
    "build:prod": "vue-tsc -b && vite build --mode production",
    "preview": "vite preview",
    "lint": "eslint src --ext .js,.ts,.vue",
    "lint:ci": "eslint --quiet src --ext .js,.ts,.vue",
    "lint:fix": "eslint src --ext .js,.ts,.vue --fix",
    "lint-staged": "eslint src --ext .js,.ts,.vue",
    "lint-style": "stylelint \"src/**/*.{vue,less,postcss,css,scss}\"",
    "lint-style:ci": "stylelint --quiet \"src/**/*.{vue,less,postcss,css,scss}\"",
    "lint-style:fix": "stylelint \"src/**/*.{vue,less,postcss,css,scss}\" --fix",
    "prepare": "husky install"
  },
  "engines": {
    "node": "20.17.0",
    "pnpm": "9.9.0"
  },
  "dependencies": {
    "@quasar/extras": "^1.16.17",
    "@sentry/vue": "^8.33.1",
    "axios": "^1.7.7",
    "pinia": "^2.2.2",
    "qs": "^6.13.0",
    "quasar": "^2.18.1",
    "vue": "^3.4.38",
    "vue-i18n": "^9.14.0",
    "vue-router": "^4.4.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.4.1",
    "@commitlint/config-conventional": "^19.4.1",
    "@quasar/vite-plugin": "^1.9.0",
    "@sentry/vite-plugin": "^2.22.5",
    "@types/node": "^22.5.2",
    "@types/qs": "^6.9.15",
    "@typescript-eslint/eslint-plugin": "5.30.6",
    "@typescript-eslint/parser": "^8.4.0",
    "@vitejs/plugin-vue": "^5.1.3",
    "eslint": "8.57.0",
    "eslint-config-standard-with-typescript": "23.0.0",
    "eslint-plugin-vue": "^9.28.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.10",
    "postcss": "^8.4.45",
    "postcss-html": "^1.7.0",
    "postcss-scss": "^4.0.9",
    "pre-commit": "^1.2.2",
    "sass": "^1.77.8",
    "sass-embedded": "^1.86.0",
    "stylelint": "^16.9.0",
    "stylelint-config-recess-order": "^5.1.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard-scss": "^13.1.0",
    "stylelint-order": "^6.0.4",
    "typescript": "^5.5.4",
    "unplugin-auto-import": "^0.18.2",
    "vite": "^5.4.2",
    "vue-tsc": "^2.1.4"
  },
  "lint-staged": {
    "*.{html,scss,css,vue}": [
      "stylelint"
    ],
    "*.{ts,js,vue}": [
      "eslint"
    ]
  }
}
